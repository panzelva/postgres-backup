{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["es2020"],
    "moduleResolution": "node",
    "importsNotUsedAsValues": "preserve",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "incremental": true,
    "noEmit": true
  },
  "include": ["src"]
}
